/* pages/canvas/canvas.wxss */
.canvasBox{
  height: 300rpx;
  position: relative;
  /* background-color: #f5f5f5; */
}
.bigCircle{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  position: absolute;
  top:0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto auto;
  background-color: rgba(221, 221, 221, 0.486);
}
.littleCircle{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  position: absolute;
  top:0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto auto;
  background-color: rgba(238, 238, 238, 0.322);
}
.canvas{
  width: 250rpx;
  height: 250rpx;
  /* position: absolute; */
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto auto;
  z-index: 99;
}